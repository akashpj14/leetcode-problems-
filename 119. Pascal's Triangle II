class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        n = [[1],[1,1]]
        if rowIndex<=1:
            return n[rowIndex]
        for i in range(1,rowIndex):  
            v = n[-1]            
            b = [1]                  
            for j in range(1, len(v)):
                b.append(v[j-1] + v[j])  
            b.append(1)              
            n.append(b)
        return n[rowIndex]
        
